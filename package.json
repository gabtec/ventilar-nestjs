{
  "name": "ventilar-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development  nest start --watch",
    "start:debug": "NODE_ENV=development  nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "separator0": "-----------------------------",
    "typeorm": "npm run build && npx typeorm -d dist/db/data-source.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run:old": "npm run typeorm -- migration:run",
    "migration:run": "npx typeorm -d dist/db/data-source.js migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:create": "typeorm migration:create",
    "separator1": "-----------------------------",
    "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create -d dist/db/data-source.js",
    "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop",
    "db:seed:old": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
    "db:seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./dist/data-source.js",
    "separator2": "-----------------------------",
    "m:clear": "npx typeorm-ts-node-commonjs -d ./src/database/data-source.ts cache:clear",
    "m:gen": "npx typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:generate ./src/database/migrations/new-def",
    "m:run": "npx typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:run",
    "m:seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js -d ./src/database/data-source.ts seed",
    "separator4": "-----------------------------",
    "prod:run": "npx typeorm-ts-node-commonjs -d ./dist/database/data-source.js migration:run",
    "prod:seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js -d ./dist/database/data-source.js seed"
  },
  "dependencies": {
    "@nestjs/common": "9.0.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/jwt": "10.0.1",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/swagger": "6.2.1",
    "@nestjs/typeorm": "9.0.1",
    "@types/bcryptjs": "2.4.2",
    "@types/passport-jwt": "3.0.8",
    "argon2": "0.30.3",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "helmet": "6.0.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "typeorm": "0.3.12",
    "typeorm-extension": "2.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.3.1",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
